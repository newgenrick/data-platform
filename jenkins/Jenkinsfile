pipeline {
  agent any

  stages {

    stage('Clean Workspace') {
      steps {
        println "calling projectsListForPR"
        sh 'printenv'
        println 'branch : ${ghprbSourceBranch}'
        sh 'git checkout origin/${ghprbSourceBranch}'
        sh 'git --no-pager diff origin/main --name-only'
      }
    }
  }
}

